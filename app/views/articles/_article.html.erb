<div id="<%= dom_id article %>">
  <p>
    <strong>Title:</strong>
    <%= article.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= article.description %>
  </p>
  <p>
    <strong>Created At:</strong>
    <%= article.created_at %>
  </p>

  <p>
    <strong>Updated At:</strong>
    <%= article.updated_at %>
  </p>
  <hr>

</div>
